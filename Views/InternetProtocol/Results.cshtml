@using Microsoft.AspNetCore.Html
<title>Тест</title>
<!DOCTYPE html>
<html>
<head>
    <style>
        body {
            margin: 0;
            display: flex;
            justify-content: center;
            flex-direction: row;
        }

        .container {
            display: flex;
            flex-direction: column;
            background-color: #ffffff;
            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.2);
            width: 1100px;
            overflow-y: auto;
        }

        .main-logo {
            position: absolute;
            top: 0;
            left: 0;
            max-width: 160px;
            max-height: 182.19px;
        }

        .heading-container {
            display: flex;
            flex-direction: row;
            justify-content: center;
            margin-bottom: 20px;
        }

        .main-heading {
            font-weight: bold;
            font-size: 60px;
            color: black;
        }

        .ip-heading {
            font-weight: bold;
            font-size: 60px;
            color: #ff6600;
        }

        .subnet-heading {
            display: flex;
            flex-direction: row;
            font-weight: bold;
            font-size: 25px;
            color: black;
        }

        .subnet-subHeading {
            margin-right: 10px;
            margin-left: 10px;
            font-weight: bold;
            font-size: 30px;
            color: #ff6600;
        }

        .mini-div-box {
            height: 40px;
            width: 75px;
            border: 1px solid #999;
            font-size: 25px;
            border-radius: 5px;
            text-align: center;
            display: flex;
            flex-direction: column;
            justify-content: center;
        }

        .info-container {
            display: flex;
            flex-direction: row;
            justify-content: center;
            margin: 20px;
        }

        .info-container-student {
            display: flex;
            flex-direction: row;
            justify-content: center;
            margin: 20px;
            margin-bottom: 200px;
        }

        .main-info-text-container {
            font-weight: bold;
            display: flex;
            flex-direction: column;
            justify-content: center;
            margin-bottom: 25px;
        }

        .info-text-container {
            font-weight: bold;
            display: flex;
            flex-direction: column;
            justify-content: center;
            margin-bottom: 5px;
        }

        .info-subContainer {
            display: flex;
            align-items: center;
            flex-direction: column;
            margin-right: 20px;
            margin-left: 20px;
            background-color: #ffffff;
            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.2);
            border-radius: 10px;
            padding: 20px;
        }

        .ip-digit-box {
            height: 38px;
            width: 23px;
            margin: 2px;
            font-size: 25px;
            text-align: center;
            border: 1px solid #999;
            display: flex;
            flex-direction: column;
            justify-content: center;
        }

        .ip-dot {
            font-weight: bold;
            font-size: 43px;
        }

        .ip-div-container {
            display: flex;
            flex-direction: row;
        }
    </style>
    @functions {
        string SplitIPDec(int octet, string ipDec)
        {
            List<string> ipOctets = ipDec.Split(".").ToList();
            return ipOctets[octet];
        }

        IHtmlContent CheckMistakesBin(char rightAnswer, char yourAnswer)
        {
            string htmlString = string.Empty;
            if (rightAnswer == yourAnswer)
            {
                htmlString = $"<span style = \"color: green;\" > {yourAnswer} </ span >";
            }
            else
            {
                htmlString = $"<span style = \"color: red;\" > {yourAnswer} </ span >";
            }
            return new HtmlString(htmlString);
        }

        IHtmlContent CheckMistakesDec(string rightAnswer, string yourAnswer)
        {
            string htmlString = string.Empty;
            if (rightAnswer == yourAnswer)
            {
                htmlString = $"<span style = \"color: green;\" > {yourAnswer} </ span >";
            }
            else
            {
                htmlString = $"<span style = \"color: red;\" > {yourAnswer} </ span >";
            }
            return new HtmlString(htmlString);
        }
    }
</head>
